@page
@model NorthwindWeb.Pages.ProductsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<div class="row">
    <h1 class="display-2">Suppliers</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Company Name</th></tr>
        </thead>
        <tbody>
            @foreach (string name in Model.Products)
            {
                <tr><td>@name</td></tr>
            }
        </tbody>
    </table>
</div>


